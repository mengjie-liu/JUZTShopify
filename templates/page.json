        "custom_liquid": "<div class=\"contentWrap\">\n<div class=\"imgWrapV verticalhalf\" id=\"img14\">\n        <div class=\"imgContainer\">\n          <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0564\/1155\/3843\/files\/JUZT.gif?v=1657127159\" alt=\"\" \/>\n        <\/div>\n      <\/div>\n<div class=\"policyEtc\">\n        <div class=\"policy gridItem smallText\">\n          <!-- <a class=\"cs hoverEffect\">CUSTOMER&nbsp;SERVICE<\/a> -->\n          <a href=\"\/policies\/shipping-policy\" class=\"sr hoverEffect\">SHIPPING&nbsp;&&nbsp;RETURNS<\/a>\n          <a href=\"\/policies\/privacy-policy\" class=\"pp hoverEffect\">PRIVACY&nbsp;POLICY<\/a>\n          <a href=\"\/policies\/terms-of-service\" class=\"tc hoverEffect\">TERMS&nbsp;&&nbsp;CONDITIONS<\/a>\n          <a href=\"\" class=\"sm hoverEffect\">SOCIAL&nbsp;MEDIA<\/a>\n        <\/div>\n      <\/div>\n<\/div>\n<style>\n.contentWrap{\n    left: var(--left);\n    position: relative;\n    height: 0;\n    width: fit-content;\n    margin-right: 0.8rem;\n    z-index: 0;\ndisplay: block;\n}\n\n@media screen and (min-width: 750px){\n#img14{\n    left: calc(var(--unit) * 2);\n    top: calc(var(--unit) * 4.5);\nz-index:1000;\n}\n.verticalhalf{\n    width: calc(var(--unit));\n    height: calc(var(--unit) * 1.5);\n    overflow: hidden;\n    margin-top: 0.8rem;\n    margin-left: -0.8rem;\n}\n.imgWrapV{\n    position: absolute;\n}\n.policyEtc{\n    position: relative;\n    margin-left: 0;\n    margin-top: 1.6rem;\n    box-sizing: border-box;\n    top: calc(var(--unit) * 4);\n    left: calc((var(--left)) * -1);\n    height: calc(100vh);\nwidth: 100vw;\nbackground-color:black;\n\n}\n.cs{\ncolor:white !important;\n    position: absolute;\n    left: calc(var(--left) + 1.6rem);\n    bottom:4.3rem;\n    cursor: pointer;\n}\n.sr{\ncolor:white !important;\n    cursor: pointer;\n    \/* padding-left:1em; *\/\n}\n.pp{\ncolor:white !important;\n    padding-left: calc(var(--unit) \/ 2);\n    cursor: pointer;\n    \/* padding-left:1em; *\/\n \n}\n.tc{\ncolor:white !important;\n    padding-left: calc(var(--unit) \/ 2);\n    cursor: pointer;\n    \/* padding-left:1em; *\/\n \n}\n.sm{\ncolor:white !important;\n    padding-left: calc(var(--unit) \/ 2);\n    cursor: pointer;\n    \/* padding-left:1em; *\/\n}\n.policy{\nposition: absolute;\nbottom: 4.3rem;\nleft: 50%;\ntransform: translateX(-50%);\nwidth: 100vw;\ntext-align: center;\n}\n}\n\n@media screen and (max-width: 749px){\n#img14{\n display: none;\n}\n.policyEtc{\n    position: absolute;\n    margin-left: 0;\n    margin-top: 0.8rem;\n    box-sizing: border-box;\n    top: calc(var(--unit) * 18);\n    left: calc((var(--left)) * -1);\n    height: calc(100vh - var(--unit) * 3 - 0.8rem);\nwidth: 100vw;\nbackground: black;\n}\n.cs, .sr, .pp, .tc, .sm{\ncursor: pointer;\npadding-left: calc(var(--left));\nbottom:4.3rem;\ncolor: white !important;\ndisplay: block;\n}\n}\n<\/style>",
