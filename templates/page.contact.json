r;\n}\n\n@media screen and (min-width: 1700px){\n#div1, #div2, #div4, #div5, #div5{\nfont-size: 1.5em;\n}\n#div2{\nfont-size: 1.2em;\n}\n}\n<\/style>\n\n<script>\n  $(document).ready(function () {\n  window.scrollTo(0, 0);\n\n  let idleTimer = null;\n  let idleState = false;\n\n  function showImg(time) {\n    clearTimeout(idleTimer);\n    if (idleState == true) {\n      $(\".aboutImage\").css(\"opacity\",1);\n    }\n    idleState = false;\n    idleTimer = setTimeout(function () {\n      $(\".aboutImage\").css(\"opacity\",0);\n      idleState = true;\n    }, time);\n  }\n\n  showImg(800);\n\n  $(window).mousemove(function () {\n    showImg(800);\n  });\n  $(window).scroll(function () {\n    showImg(800);\n  });\n});\n<\/script>",
